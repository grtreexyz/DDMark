/* pages/history/history.wxss */
.title{
    width:100%;
    text-align: center;
    font-weight: bold;
    padding: 20px;
    box-sizing: border-box;
}
.table{
    width:100%;
}

.tr{
    width:100%;
    height: 100px;
    display: flex;
    border: 1px solid #eee;
}
.td{
    flex: 1;
    height:100px;
    border: 1px solid #eee;
    text-align: center;
    line-height: 50px;
    font-size:14px; 
}

.trtitle{
    width:100%;
    display: flex;
    height:20px;
    border: 1px solid #eee;
}
.trtitle .td{
    height: 20px;
    line-height: 20px;
    font-size:14px; 
}